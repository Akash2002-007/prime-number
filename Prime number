public class PrimeNo {
    public static void main(String[] args){
        int no=89, temp=0;
        for(int i=2;i<=no-1;i++){
            if(no%2==0){
                temp=temp+1;
            }
            if(temp>0){
                System.out.println("not the Prime number");
            }
        else{
            System.out.println(" prime number");
            }
        }
    }


}
